{
  "name": "frame-web-for-js",

  "version": "2.3.8",


  "scripts": {
    "ng": "ng",
    "start": "ng serve --host=127.0.0.1",
    "build": "ng build --configuration production --base-href \"https://frameweb.malme.app/\" && npm run filecopy3 && angular-cli-ghpages",
    "build-action": "ng build --configuration production --base-href \"https://frameweb.malme.app/\" && npm run filecopy3",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:dev": "concurrently \"ng build --delete-output-path=false --watch\" \"webpack --config webpack.electron.dev.config.js --watch\"",
    "start:dev": "electron dist/shell.js",
    "electron:windows": "npm run build:prod && npx electron-builder build --windows",
    "build:prod": "ng build --configuration production --optimization --extract-licenses --build-optimizer && webpack --config webpack.electron.prod.config.js && npm run filecopy1 && npm run filecopy2",
    "start:prod": "electron dist/index.js",
    "filecopy1": "node -e \"require('fs').copyFileSync('dev-app-update.yml', 'dist/dev-app-update.yml')\"",
    "filecopy2": "node -e \"require('fs').copyFileSync('app-update.yml', 'dist/app-update.yml')\"",
    "filecopy3": "node -e \"require('fs').copyFileSync('src/404.html', 'dist/404.html')\""
  },
  "description": "FrameWebforJS",
  "author": {
    "name": "MALME INC.",
    "email": "support@structuralengine.com"
  },
  "keywords": [
    "angular",
    "angular 15",
    "typescript",
    "sass"
  ],
  "private": true,
  "dependencies": {
    "@angular-devkit/schematics": "^15.2.7",
